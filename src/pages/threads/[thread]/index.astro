---
import { threads } from '../../../lib/threads';
export async function getStaticPaths() {
return threads.map((thread) => ({
params: { thread: thread },
}));
}
const { thread } = Astro.params;
---
<html lang="en">
<head>
	<meta charset="utf-8">
	<title>{thread}</title>
	<meta http-equiv="refresh" content={`0;url=/threads/${thread}/1`} />
	<link rel="canonical" href={`/threads/${thread}/1`} />
</head>
<body>
<p>Redirecting to <a href={`/threads/${thread}/1`}>/threads/{thread}/1</a>â€¦</p>
</body>
</html>